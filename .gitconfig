[user]
  name = "Bohdan"
  useConfigOnly = true
  email = b.liesnikov+git@gmail.com
  signingkey = ssh-rsa 
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true

[gpg]
  format = ssh

[gpg "ssh"]
  program = "/opt/1Password/op-ssh-sign"

[commit]
  gpgsign = true
